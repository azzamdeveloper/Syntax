fun main(){
    val nilaiMatematika = 88
    val nilaiInggris = 93
    val nilaiSejarah = 75
    
    predikat(nilaiMatematika)
    predikat(nilaiInggris)
    predikat(nilaiSejarah)
}

fun predikat(nilai: Int){
    when(nilai){
        in 90..100 -> println("Predikat A dengan nilai : $nilai")
        in 80..89 -> println("Predikat B dengan nilai : $nilai")
        in 70..79 -> println("Predikat C dengan nilai : $nilai")
        else -> println("Predikat D dengan nilai : $nilai")
    }
}
